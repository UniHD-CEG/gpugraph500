reset
unset key
set key outside
set grid y

set title "Influence of the scale factor to the TEPS rate at different cluser sizes"

set ylabel "TEPS"
set xlabel "Nodes"

set key autotitle columnhead
set terminal jpeg size 1024,576
set output "teps_node_scale.jpg"

scales="$scales"
filename(n) = sprintf("$datpath/scale_%s.dat", n)
plot for [scale in scales ] filename(scale) using 1:3 title sprintf("Scale  %s", scale)	with linespoints				
